#php_value register_globals                1
#php_value memory_limit                640M
#php_value session.cookie_lifetime 21600
#php_value session.gc_maxlifetime 21600
RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www\.|)lingocasa\.com
RewriteCond %{REQUEST_URI} ^/$
RewriteRule ^(.*)$ http://www.lingocasa.com/index.php [R=301,L]
AddCharset ISO-8859-2 .html
Header set Content-Type "text/html; charset=ISO-8859-2"


#RewriteCond %{HTTPS} off
#RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]
